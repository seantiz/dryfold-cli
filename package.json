{
    "name": "dryfold-cli",
    "version": "0.8.5",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "folddry": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsx",
        "start": "node --experimental-specifier-resolution=node dist/index.js",
        "dev": "tsx src/index.ts"
    },
    "devDependencies": {
        "@types/node": "^22.13.2",
        "tree-sitter": "0.22.1",
        "tree-sitter-cpp": "^0.23.4",
        "tsx": "^4.19.3",
        "typescript": "^5.7.3"
    },
    "packageManager": "pnpm@9.1.4",
    "dependencies": {
        "@hpcc-js/wasm-graphviz": "^1.7.0"
    },
    "pnpm": {
        "peerDependencyRules": {
            "allowedVersions": {
                "tree-sitter": "^0.22.1"
            }
        }
    }
}
